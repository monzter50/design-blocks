{
  "name": "@design-blocks/native",
  "version": "0.1.0-next.0",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "source": "src/index.ts",
  "typings": "dist/typescript/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:wootsbot/design-blocks.git",
    "directory": "native"
  },
  "scripts": {
    "build:native": "bob build",
    "clean": "yarn clean:build && rimraf -rf .turbo",
    "clean:build": "rimraf -rf dist",
    "build": "yarn clean:build && yarn build:native",
    "lint:types": "tsc --noEmit",
    "bundlesize": "bundlesize"
  },
  "keywords": [],
  "bundlesize": [
    {
      "path": "./dist/commonjs/index.js",
      "maxSize": "1kB"
    },
    {
      "path": "./dist/module/index.js",
      "maxSize": "1kB"
    },
    {
      "path": "./dist/typescript/index.d.ts",
      "maxSize": "0.5kB"
    }
  ],
  "dependencies": {
    "@design-blocks/primitives": "workspace:*",
    "@design-blocks/system": "workspace:*",
    "@design-blocks/theme": "workspace:*",
    "@design-blocks/types": "workspace:*",
    "@design-blocks/utils": "workspace:*"
  },
  "devDependencies": {
    "bundlesize": "0.18.1",
    "react-native-builder-bob": "0.20.3",
    "rimraf": "4.1.2",
    "tsconfig": "workspace:*",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "@emotion/native": "11.10.6",
    "@emotion/react": "11.10.6",
    "@types/react": "*",
    "@types/react-native": "*",
    "react": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "@emotion/native": {
      "optional": true
    },
    "@emotion/react": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
