{
  "name": "@design-blocks/core",
  "version": "1.1.0-beta.3",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "types": "./types/index.d.ts",
  "typings": "./types/index.d.ts",
  "react-native": "dist/commonjs/index.js",
  "source": "src/index.ts",
  "files": [
    "native",
    "dist",
    "src",
    "types/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:design-blocks/design-blocks.git",
    "directory": "@blocks-core"
  },
  "scripts": {
    "build:native": "bob build",
    "clean": "pnpm clean:build && rimraf -rf .turbo",
    "clean:build": "rimraf -rf dist",
    "lint:types": "tsc --noEmit",
    "bundlesize": "bundlesize",
    "build": "pnpm clean:build && pnpm build:native"
  },
  "keywords": [],
  "bundlesize": [
    {
      "path": "./src/index.ts",
      "maxSize": "0.5kB"
    },
    {
      "path": "./src/index.ts",
      "maxSize": "0.5kB"
    },
    {
      "path": "./dist/typescript/index.d.ts",
      "maxSize": "0.5kB"
    }
  ],
  "dependencies": {
    "@design-blocks/theme": "workspace:*",
    "css-to-react-native": "3.2.0"
  },
  "devDependencies": {
    "@types/react": "18.0.28",
    "bundlesize": "0.18.1",
    "react": "^18",
    "react-native": "0.72",
    "react-native-builder-bob": "0.22.0",
    "rimraf": "4.1.2",
    "tsconfig": "workspace:*",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.71"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
