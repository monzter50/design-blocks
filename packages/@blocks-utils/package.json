{
  "name": "@design-blocks/utils",
  "version": "1.0.0-beta.15",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "dist",
    "camel-case.d.ts",
    "compact.d.ts",
    "deepmerge.d.ts",
    "get.d.ts",
    "is-dev.d.ts",
    "is-null-or-undefined.d.ts",
    "is-number.d.ts",
    "is-object.d.ts",
    "is-string.d.ts",
    "is-undefined.d.ts",
    "remove-empty-objects.d.ts",
    "spacing.d.ts",
    "to-pixels.d.ts"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./camel-case": "./dist/camelCase.js",
    "./compact": "./dist/compact.js",
    "./deepmerge": "./dist/deepmerge.js",
    "./get": "./dist/get.js",
    "./is-dev": "./dist/isDev.js",
    "./is-null-or-undefined": "./dist/isNullOrUndefined.js",
    "./is-number": "./dist/isNumber.js",
    "./is-object.": "./dist/isObject.js",
    "./is-string": "./dist/isString.js",
    "./is-undefined": "./dist/isUndefined.js",
    "./remove-empty-objects": "./dist/removeEmptyObjects.js",
    "./spacing": "./dist/spacing.js",
    "./to-pixels": "./dist/toPixels.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:design-blocks/design-blocks.git",
    "directory": "@blocks-utils"
  },
  "scripts": {
    "build": "pnpm clean:build && tsc -p tsconfig.build.json",
    "build:native": "bob build",
    "clean": "pnpm clean:build && rimraf -rf .turbo",
    "clean:build": "rimraf -rf dist",
    "lint": "../../node_modules/.bin/biome check src",
    "lint:fix": "../../node_modules/.bin/biome check --apply-unsafe src",
    "bundlesize": "bundlesize"
  },
  "keywords": [],
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "0.5KB"
    },
    {
      "path": "./dist/index.d.ts",
      "maxSize": "0.5kB"
    }
  ],
  "dependencies": {
    "ramda": "0.28.0"
  },
  "devDependencies": {
    "@types/node": "18.13.0",
    "@types/ramda": "0.28.23",
    "bundlesize": "0.18.1",
    "react-native-builder-bob": "0.23.1",
    "rimraf": "4.1.2",
    "tsconfig": "workspace:*",
    "typescript": "5.0.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
